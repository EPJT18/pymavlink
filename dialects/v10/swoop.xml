      <entry value="8192" name="BAD_GPS_POSITION"/> <!-- 13 -->
      <entry value="16384" name="GPS_NOT_HEALTHY"/> <!-- 14 -->
    
      <!--<entry value="1" name="GPS_POSITIONS_DIFFERENT"/>  --><!-- 15 -->
      <!--<entry value="2" name="GPS_BLENDING_UNHEALTHY"/>--><!-- 16 -->
      <!--<entry value="4" name="GPS_DIFFERENT_TO_EKF"/>--><!-- 17 -->
      <!--<entry value="8" name="BAD_PARAMS"/>--><!-- 18 -->
      <!--<entry value="16" name="BATTERY_PROBLEM"/>--><!-- 19 There are > 100 sub variants of these, but not much point when we dont use them-->
      <!--<entry value="1024" name="LOGGING_FAILED"/>--> <!-- 10 -->
