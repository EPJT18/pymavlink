      <!--<entry value="8192" name="NO_MISSION_LOADED"/>--> <!-- 13 -->

    </enum>
    <enum name="SWOOP_AUTO_ACTION_FLAGS">
      <description>Flag bits to indicate when a failure event has been detected</description>
      <entry value="1" name="HOVER_ASSIST_TRIGGERED"/>
      <entry value="2" name="HOVER_ASSIST_REASON_ALT"/>
