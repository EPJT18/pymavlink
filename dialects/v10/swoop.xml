  <messages>
    <message id="6901" name="SWOOP_FLAGS">
      <description>Periodic Status Flag Packet</description>
      <field type="uint32_t" name="armingFlags" enum="SWOOP_ARMING_CHECK_FLAGS">Flag bits to indicate which of the arming checks is failing</field>
      <field type="uint32_t" name="actionFlags" enum="SWOOP_AUTO_ACTION_FLAGS">Flag bits to indicate when a failure event has been detected</field>
    </message>
    
  </messages>
</mavlink>
